@model List<AppUser>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Kullanıcı Adı</th>
            <th scope="col">Mail Adresi</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.UserName</th>
                <th>@item.Email</th>
            </tr>
        }
    </tbody>
</table>